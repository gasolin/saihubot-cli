{
	"name": "sai",
	"version": "0.13.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"setup-submodule": "git submodule update --init --recursive",
		"start": "babel-node --presets @babel/preset-env src/cli.js",
		"build": "babel ./src --out-dir ./dist --copy-files",
		"test": "xo && ava",
		"update-saihubot": "git submodule foreach --recursive git pull origin gh-pages"
	},
	"dependencies": {
		"dedent": "^0.7.0",
		"execa": "^4.0.3",
		"import-jsx": "^4.0.0",
		"ink": "^3.0.8",
		"ink-markdown": "^1.0.1",
		"ink-select-input": "^4.2.0",
		"meow": "^7.1.1",
		"react": "^17.0.1",
		"saihubot": "^0.14.0"
	},
	"devDependencies": {
		"@ava/babel": "^1.0.1",
		"@babel/cli": "^7.12.1",
		"@babel/core": "^7.12.3",
		"@babel/node": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"@babel/register": "^7.12.1",
		"ava": "^3.13.0",
		"chalk": "^4.1.0",
		"eslint-config-xo-react": "^0.23.0",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"ink-testing-library": "^2.1.0",
		"xo": "^0.34.1"
	},
	"ava": {
		"babel": true,
		"require": [
			"@babel/register"
		]
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	}
}
